<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for Notice -->
        <record id="sr_sequence_notice" model="ir.sequence">
            <field name="name">Notice Sequence</field>
            <field name="code">sr.sequence.notice</field>
            <field name="prefix">NOTICE/%(range_year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
