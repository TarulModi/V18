<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <record id="seq_rma_order" model="ir.sequence">
        <field name="name">RMA Order</field>
        <field name="code">rma.order</field>
        <field name="prefix">RMA-</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_rma_supplier" model="ir.sequence">
        <field name="name">RMA Supplier</field>
        <field name="code">rma.supplier</field>
        <field name="prefix">SRMA-</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
