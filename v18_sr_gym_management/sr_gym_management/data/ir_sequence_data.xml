<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="seq_notice_boards" model="ir.sequence">
        <field name="name">Notice Boards</field>
        <field name="code">notice.boards</field>
        <field name="prefix">NB</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_student_admission" model="ir.sequence">
        <field name="name">Admission</field>
        <field name="code">student.admission</field>
        <field name="prefix">AD</field>
        <field name="padding">9</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_student_admission" model="ir.sequence">
        <field name="name">Admission</field>
        <field name="code">student.admission</field>
        <field name="prefix">AD</field>
        <field name="padding">9</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_enrollment" model="ir.sequence">
        <field name="name">Enrollment Sequence</field>
        <field name="code">student.enrollment</field>
        <field name="prefix">ENR/%(year)s/</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="implementation">no_gap</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
